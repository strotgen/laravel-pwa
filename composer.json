{
    "name": "erag/laravel-pwa",
    "description": "A simple and easy-to-use PWA (Progressive Web App) package for Laravel applications.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "pwa",
        "progressive web app",
        "offline",
        "service worker",
        "manifest",
        "laravel package",
        "laravel pwa"
    ],
    "authors": [
        {
            "name": "Er Amiit Gupta",
            "email": "info.eramitgupta@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/pint": "^1.13", 
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.12|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "files": [
            "src/Pwahelpers.php"
        ],
        "psr-4": {
            "EragLaravelPwa\\": "src/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "EragLaravelPwa\\EragLaravelPwaServiceProvider"
            ],
            "aliases": {
                "Pwa": "EragLaravelPwa\\Facades\\Pwa"
            }
        }
    }
}
